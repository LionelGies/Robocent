$("#recordings-table .ajax-loading").hide();

<% if @recordings.present? %>
  <% @recordings.each do |rec| %>
    $("#recordings-table table tbody").prepend("<%= escape_javascript(render :partial => "recording_tiny", :locals => {:rec => rec}).html_safe %>");
  <% end %>
    $("#last_loaded_rec").val(<%= @recordings.last.id %>);
<% end %>