<% if @dnc_list.present? and @destroyed.present? %>

$("#dnc_row_<%= @dnc.id %>").remove();
$("#dnc-table .ajax-loading").hide();

<% else %>

  <% unless @destroyed.blank? %>
    $("#contacts-table tbody tr#contact_row_<%= @contact.id %>").replaceWith("<%= escape_javascript(render :partial => "contacts/contact_tiny", :locals => {:contact => @contact}).html_safe %>");
  <% end %>
  $("#contacts-table .ajax-loading").hide();

<% end %>